{
    "version": "0.2",
    "language": "en",
    "languageSettings": [
        {
            "languageId": "markdown",
            "ignoreRegExpList": [
                "CodeBlock",
                "CodeInline"
            ],
            "patterns": [
                {
                    "name": "CodeBlock",
                    "pattern": "```[\\w\\W]+?```"
                },
                {
                    "name": "CodeInline",
                    "pattern": "`[\\w\\W]+?`"
                }
            ]
        }
    ],
    "ignorePaths": [
        "*.svg"
    ],
    "words": [
        "barebones",
        "dev",
        "devs",
        "Dioxus",
        "Dioxus's",
        "haptic",
        "haptics",
        "jank",
        "Makepad",
        "Makepad's",
        "mgmt",
        "Multiwindow",
        "performant",
        "performantly",
        "Robius",
        "Robrix",
        "Taobao",
        "toolkit",
        "toolkits",
        "ylong"
    ],
    "ignoreWords": [
        "geolocation",
        "liveview",
        "rustc",
        "rustdoc",
        "rustflags",
        "rustup",
        "SIMD",
        "struct",
        "structs",
        "syscall",
        "sysroot",
        "Thinkpad",
        "userspace",
        "webapps"
    ]
}
